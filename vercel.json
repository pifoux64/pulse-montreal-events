{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/cron/ingest",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/personalized-notifications",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/ingestion",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/recompute-taste-profiles",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/weekly-digest",
      "schedule": "0 10 * * 1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}